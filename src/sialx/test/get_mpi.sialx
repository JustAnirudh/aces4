sial get_mpi
	special list_block_map
	special print_block r
	predefined int norb
	aoindex i = 1,norb
	aoindex j = 1,norb
	local a(i,j)
	local aa(i,j)
	distributed b(i,j)
	distributed c(i,j)
	
	create b
	create c
	allocate a(*,*)
	
	println "Sending blocks"	
	do i
		do j
			a(i,j) = 42.0
			put b(i,j) = a(i,j)
			put c(i,j) += a(i,j)
			put c(i,j) += a(i,j)
		enddo j
	enddo i
	sip_barrier
	
	do i
	   do j
	      get b(i,j)
	      get c(i,j)
	      a(i,j) = b(i,j) + c(i,j)
	      execute print_block a(i,j)
	   enddo j
	enddo i
	
	sip_barrier
	execute list_block_map
	delete b
	delete c
	
endsial get_mpi