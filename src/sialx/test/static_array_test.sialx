sial static_array_test
	predefined int norb
	special fill_block_sequential wr
	special print_block r
	special print_static_array r
	
	aoindex i = 1,norb
	aoindex j = 1,norb
	static a(i,j)
	
	local b(i, j)
	
	do i
		do j
		
			execute fill_block_sequential a 1.0
			#execute print_block a(i,j)
			
			b(i, j) = a(i, j)
			
			println "execute print_block a(i,j)"
			execute print_block a(i,j)
			
			println "execute print_block b"
			execute print_block b(i, j) 
			
		enddo j
	enddo i
	
	println "execute print_block a"
	execute print_block a
	
	
	println " finished static_array_test"

endsial static_array_test
