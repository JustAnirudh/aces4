sial persistent_static_array_test2
	predefined int norb
	special fill_block_sequential wr
	special print_block r
	special print_static_array r
	aoindex i = 1:norb
	aoindex j = 1:norb
	static a[i,j]
	static b[i,j]
	
	local lb[i,j]
	allocate lb[*,*]
	
	do i
		do j
			execute fill_block_sequential a[i,j] 1.0
			println "execute print_block a(i,j)"
			execute print_block a[i,j]
		enddo j
	enddo i
	
	println "execute print_block a"
	execute print_block a
	
	restore_persistent b "array_a"
	println "execute print_block b"
	execute print_block b
	
	do i
		do j
			lb [i, j] = b[i, j]
		enddo j
	enddo i
	
	println " finished persistent_static_array_test2"

endsial persistent_static_array_test2